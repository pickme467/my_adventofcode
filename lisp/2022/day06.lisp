(defun diff-index-4 (input)
  (loop for (a b c d) on input by #'cdr
        for iter = 4 then (1+ iter)
        when (not (or (equal a b) (equal a c) (equal a d) (equal b c) (equal b d) (equal c d)))
          return iter))

(defun diff-index-14 (input)
  (loop for (a b c d e f g h i j k l m n) on input by #'cdr
        for iter = 14 then (1+ iter)
        when (equal 14 (length (remove-duplicates (list a b c d e f g h i j k l m n))))
          return iter))

(defun day06-part1 ()
  (assert (equal 1929 (diff-index-4 (input)))))

(defun day06-part2 ()
  (assert (equal 3298 (diff-index-14 (input)))))

(defun input ()
  (loop for i across "" collecting i))
