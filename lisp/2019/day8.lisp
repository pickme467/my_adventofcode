(defun split-to-layers (data width height)
  (let ((layer-size (/ (length data) (* width height)))
        (layers ()))
    (dotimes (l layer-size)
      (let ((layer ()))
        (dotimes (h height)
          (let ((layer-part (subseq data
                                    (* (+ (* l height) h) width)
                                    (* (+ (* l height) (1+ h))  width))))
            (push layer-part layer)))
        (push (reverse layer) layers)))
    (reverse layers)))

(defun find-fewest-zeros (data)
  (let ((fewest-zeroes
         (map 'list
              #'(lambda (layer)
                  (list
                   (reduce #'(lambda (acc string)
                               (+ acc (count #\0 string))) layer :initial-value 0)
                   layer)) data)))
    (setf fewest-zeroes (sort fewest-zeroes #'< :key #'car))
    fewest-zeroes))

(defun multiply-ones-and-twos (fewest-zeroes)
  (let ((layer (nth 1 (car fewest-zeroes))))
    (let ((ones (reduce #'(lambda (acc string)
                            (+ acc (count #\1 string))) layer :initial-value 0))
          (twos (reduce #'(lambda (acc string)
                            (+ acc (count #\2 string))) layer :initial-value 0)))
      (* ones twos))))

(defun reduce-layers (data)
  (let ((output (copy-list (car data))))
    (dolist (l  data)
      (dotimes (n (length l))
        (let ((output-row (nth n output))
              (candidate-row (nth n l)))
          (dotimes (x (length output-row))
            (if (equal #\2 (elt output-row x))
                (setf (elt output-row x) (elt candidate-row x)))))))
    output))

(defun present (data)
  (dolist (line data)
    (format t "~a~%" (map 'string #'(lambda (x) (if (equal #\0 x) #\   #\#)) line))))

