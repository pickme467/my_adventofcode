(defun count-broken-missing (input)
  (loop for line in input for broken = (broken line)
        when (car  broken) sum (to-illegal-value (car broken)) into broken-sum
          else  collect (cadr broken) into missing
        finally (return (list broken-sum (get-middle (count-missing-value missing))))))

(defun broken (line)
  (loop for c across line with stack = ()
        do (if (is-opening c)
               (push (closing c) stack)
               (let ((closing (pop stack)))
                 (when (not (equal closing c)) (return (list c ())))))
        finally (return (list nil stack))))

(defun is-opening (c)
  (member c '(#\( #\[ #\{ #\<)))

(defun closing (c)
  (cond ((equal c #\() #\))
        ((equal c #\[) #\])
        ((equal c #\{) #\})
        ((equal c #\<) #\>)))

(defun to-illegal-value (c)
  (cond ((equal c #\)) 3)
        ((equal c #\]) 57)
        ((equal c #\}) 1197)
        ((equal c #\>) 25137)))

(defun to-missing-value (c)
  (cond ((equal c #\)) 1)
        ((equal c #\]) 2)
        ((equal c #\}) 3)
        ((equal c #\>) 4)))

(defun count-missing-value (list)
  (loop for n in list
        collect
        (loop for k in n
              sum (+ (to-missing-value k) (* 5 result) (- result)) into result
              finally (return result))))

(defun get-middle (list)
  (let ((middle-index (/ (1- (length list)) 2)))
    (nth middle-index (sort list #'<))))

(defun day10-1 () (assert (= 268845 (car (count-broken-missing (input))))))

(defun day10-2 () (assert (= 4038824534 (cadr (count-broken-missing (input))))))

